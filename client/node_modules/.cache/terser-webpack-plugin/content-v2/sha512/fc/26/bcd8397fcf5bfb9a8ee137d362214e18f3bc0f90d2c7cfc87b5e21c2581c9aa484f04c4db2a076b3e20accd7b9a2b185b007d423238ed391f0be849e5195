Task }) => {\n  const [mnueItem, setMnueItem] = useState(1);\n  const [editTaskToggle, setEditTaskToggle] = useState(false);\n  const [deleteTaskToggle, setDeleteTaskToggle] = useState(false);\n  const [editedTask, setEditedTask] = useState(task);\n\n  let component = null;\n  switch (mnueItem) {\n    case 1:\n      component = (\n        <>\n          <Task\n            key={task.id}\n            task={task}\n            deleteTask={deleteTask}\n            toggleReminder={toggleReminder}\n            setMnueItem={setMnueItem}\n          />\n          <div width=\"150px\">\n            <div>\n              <FaEdit\n                className=\"icon\"\n                style={{\n                  cursor: \"pointer\",\n                  fontSize: \"12px\",\n                  marginRight: \"10px\",\n                  paddingBottom: \"2px\",\n                }}\n                onClick={() => setMnueItem(2)}\n              />\n              <FaTimes\n                className=\"icon\"\n                style={{ cursor: \"pointer\" }}\n                onClick={() => setMnueItem(3)}\n              />\n            </div>\n          </div>\n        </>\n      );\n      break;\n    case 2:\n      component = (\n        <>\n          <TaskEdit\n            key={task.id}\n            task={task}\n            deleteTask={deleteTask}\n            toggleReminder={toggleReminder}\n            editedTask={editedTask}\n            setEditedTask={setEditedTask}\n            setMnueItem={setMnueItem}\n            editTask={editTask}\n          />\n          <span margin=\"10\">\n            <FaCheck\n              className=\"icon\"\n              style={{ fontSize: \"10\" }}\n              onClick={() => {\n                setMnueItem(1);\n                editTask(editedTask);\n              }}\n            />\n          </span>\n        </>\n      );\n      break;\n    case 3:\n      component = <DeleteTask deleteTask={deleteTask} task={task} setMnueItem={setMnueItem}/>;\n      break;